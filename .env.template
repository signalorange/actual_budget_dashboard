# Actual Budget HTTP API Configuration
# Copy this file to .env and fill in your actual values

# URL of your Actual Budget HTTP API instance
ACTUAL_HTTP_API_URL=http://localhost:5007

# API key for the HTTP API (generate with: apg -m 50 -n 1)
ACTUAL_HTTP_API_KEY=your_api_key_here

# Optional: Enable verbose logging
ACTUAL_VERBOSE=false
